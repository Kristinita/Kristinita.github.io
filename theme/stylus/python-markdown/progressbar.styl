/* Progress Bars */

.progress
	display block
	width 300px
	margin 10px 0
	height 24px
	border-radius 3px
	background-color SashaColor1
	position relative
	box-shadow inset -1px 1px 3px SashaColor2

.progress-label
	position absolute
	text-align center
	font-weight bold
	width 100%
	margin 0
	line-height 24px
	color SashaColor3
	text-shadow 1px 1px 0 SashaColor4, -1px -1px 0 SashaColor4, -1px 1px 0 SashaColor4, 1px -1px 0 SashaColor4, 0 1px 0 SashaColor4, 0 -1px 0 SashaColor4, 1px 0 0 SashaColor4, -1px 0 0 SashaColor4, 1px 1px 2px SashaColor5
	-webkit-font-smoothing antialiased
	white-space nowrap
	overflow hidden

.progress-bar
	height 24px
	float left
	border-radius 3px
	background-color SashaColor6
	box-shadow inset 0 1px 0 SashaColor7, inset 0 -1px 0 SashaColor2
	background-size 30px 30px
	background-image linear-gradient(
    135deg,
    rgba(255, 255, 255, .4) 27%, transparent 27%,
    transparent 52%, rgba(255, 255, 255, .4) 52%,
    rgba(255, 255, 255, .4) 77%, transparent 77%,
    transparent
  )

.progress-100plus
	.progress-bar
		background-color SashaColor8

.progress-80plus
	.progress-bar
		background-color SashaColor9

.progress-60plus
	.progress-bar
		background-color SashaColor10

.progress-40plus
	.progress-bar
		background-color SashaColor11

.progress-20plus
	.progress-bar
		background-color SashaColor12

.progress-0plus
	.progress-bar
		background-color SashaColor13

.candystripe-animate
	.progress-bar
		-webkit-animation animate-stripes 3s linear infinite
		animation animate-stripes 3s linear infinite

@-webkit-keyframes animate-stripes
	0%
		background-position 0 0

	100%
		background-position 60px 0

@keyframes animate-stripes
	0%
		background-position 0 0

	100%
		background-position 60px 0

.gloss
	.progress-bar
		box-shadow inset 0 4px 12px SashaColor14,
    inset 0 -12px 0 SashaColor15

