SashaColor1 = #ededed
SashaColor2 = rgba(0, 0, 0, .1)
SashaColor3 = #333
SashaColor4 = #fefefe
SashaColor5 = #000
SashaColor6 = #96c6d7
SashaColor7 = rgba(255, 255, 255, .5)
SashaColor8 = #a6d796
SashaColor9 = #c6d796
SashaColor10 = #d7c896
SashaColor11 = #d7a796
SashaColor12 = #d796a6
SashaColor13 = #c25f77
SashaColor14 = rgba(255, 255, 255, .7)
SashaColor15 = rgba(0, 0, 0, .05)

